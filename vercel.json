{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/client/(.*)", "dest": "/routes/client.js" },
      { "src": "/api/event/(.*)", "dest": "/routes/event.js" },
      { "src": "/(.*)", "dest": "index.js" }
    ]
}